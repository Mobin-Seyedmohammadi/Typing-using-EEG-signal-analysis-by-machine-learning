   "cell_type": "code",
   "execution_count": 3,
   "id": "fdfdb117",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_fisher_score(data):\n",
    "    \n",
    "    data1 = data[data[data.columns[-1]]==0]  #Samples with label 0\n",
    "    data2 = data[data[data.columns[-1]]==1]  #Samples with label 1\n",
    "    var1 = data1.var()\n",
    "    var2 = data2.var()\n",
    "    mean_dif1 = data1.mean() - data.mean()\n",
    "    mean_dif2 = data2.mean() - data.mean()\n",
    "    n1 = len(data1)\n",
    "    n2 = len(data2)\n",
    "    return (n1*(mean_dif1**2) + n2*(mean_dif2**2)) / (n1 * var1 + n2 * var2)[:-1] \n",
    "#-----------------------------------------------------------------------------------------------------\n",
    "def CV(X, Y, estimator, folds):#define cross validation function\n",
    "    score=[]\n",
    "    cms = np.zeros((2, 2))\n",
    "    for i in range(folds):\n",
    "        \n",
    "        test_number = len(X)/ folds  #####The number of samples in each fold\n",
    "        b_range = int(test_number * i)\n",
    "        e_range = int(test_number * (i+1))\n",
    "        \n",
    "        if i == folds - 1 :\n",
    "            e_range = -1\n",
    "        \n",
    "        X_test = np.array(X.iloc[b_range : e_range])\n",
    "        Y_test = np.array(Y.iloc[b_range : e_range])\n",
    "        X_train = np.array(X.drop(X.index[b_range : e_range]))\n",
    "        Y_train = np.array(Y.drop(Y.index[b_range : e_range]))\n",
    "        \n",
    "        model = estimator.fit(X_train, Y_train)\n",
    "        pred = model.predict(X_test)\n",
    "        accuracy = model.score(X_test, Y_test)\n",
    "        roc_auc = roc_auc_score(Y_test, pred)\n",
    "        cm = confusion_matrix(Y_test, pred)\n",
    "        TN, FP, FN, TP = cm.ravel()\n",
    "        cms +=np.array(cm)\n",
    "        sensitivity = TP / (TP + FN)\n",
    "        specificity = TN / (TN + FP)\n",
    "        score.append(np.array([accuracy, roc_auc, sensitivity, specificity]))      \n",
    "    \n",
    "    score =  np.array(score).mean(axis = 0)\n",
    "    \n",
    "    return score, cms\n",
    "#-----------------------------------------------------------------------------------------------------\n",
    "def select_data(data, fisher_score, number):\n",
    "    best_features_index = (-np.array(fisher_score)).argsort()[:number] #getting index of our best features\n",
    "    new_data = data[best_features_index]\n",
    "    return new_data\n",
    "#-----------------------------------------------------------------------------------------------------\n",
    "def SC_WD(prob,letter) : #\"Single Character\" word detector\n",
    "    detected_word = str()\n",
    "    for j in range(5):  #split our samples to 5 parts, as we need 5 letters\n",
    "        word_predict = [0]*37 \n",
    "        size = int(len(prob) / 5)\n",
    "        for i in range(size*j,size*(j+1)):\n",
    "            index = int(letter[i])\n",
    "            word_predict[index] += (prob[i][1]) \n",
    "        word = (-np.array(word_predict)).argsort()[0] \n",
    "        detected_word += find_letter(word)\n",
    "    return detected_word\n",
    "#-----------------------------------------------------------------------------------------------------\n",
    "def RC_WD(prob,letter) : #'Row-Column' word detector\n",
    "    detected_word = str()\n",
    "    for j in range(5): #split our samples to 5 parts, as we need 5 letters\n",
    "        word_predict = [0]*37\n",
    "        size = int(len(prob) / 5)\n",
    "        for i in range(size*j,size*(j+1)):\n",
    "            index = int(letter[i])\n",
    "            word_predict[index] += (prob[i][1])\n",
    "        detected_column = (-np.array(word_predict[1:7])).argsort()[0]\n",
    "        detected_row = (-np.array(word_predict[7:])).argsort()[0]\n",
    "        word = detected_row * 6  + detected_column +1\n",
    "        detected_word += find_letter(word)\n",
    "    return detected_word\n",
    "#-----------------------------------------------------------------------------------------------------\n",
    "def find_letter(x): #using ascii code to finde letters\n",
    "    x = int(x)\n",
    "    if x == 0 :\n",
    "        return str('')\n",
    "    if x < 27:\n",
    "        return chr(x + 64)\n",
    "    return chr(x +21)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "b83d6120",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Data:\n",
    "    def __init__(self, name, num):\n",
    "        self.name = name #train or test\n",
    "        self.num = str(num) #person number\n",
    "        print('Loading ' + self.name + self.num)\n",
    "        self.load_file()\n",
    "        clear_output(wait=False)\n",
    "        print('getting information of ' + self.name + self.num)\n",
    "        self.get_information() # triggers time(pulses), labels, letters, triggers which were true(true pulses) \n",
    "        clear_output(wait=False)\n",
    "    #-----------------------------------------------------------------------------------------------------        \n",
    "    def is_train(self): # detecting if it is train data or not\n",
    "        if self.name == 'TrainData':\n",
    "            return True\n",
    "        return False\n",
    "    #-----------------------------------------------------------------------------------------------------        \n",
    "    def load_file(self): #load file\n",
    "        data_dir = pjoin(os.getcwd(), 'Dataset', 'Model' + self.num)\n",
    "        mat_fname = pjoin(data_dir, self.name + self.num + '.mat')\n",
    "        annots = loadmat(mat_fname)\n",
    "        con_list = [[element for element in upperElement] for upperElement in annots[self.name + self.num]]\n",
    "        newData = list(element for element in con_list)\n",
    "        columns = list(i for i in range(annots[self.name + self.num].shape[1]))\n",
    "        self.data = pd.DataFrame(newData, columns=columns)\n",
    "    #-----------------------------------------------------------------------------------------------------        \n",
    "    def get_information(self):\n",
    "        self.pulses = []\n",
    "        self.True_pulses = []\n",
    "        self.label = []\n",
    "        self.letter = []\n",
    "        \n",
    "        for i in tqdm(range(1,self.data.shape[1])):\n",
    "            if self.data.iloc[9][i] > self.data.iloc[9][i-1]: # if it is true, then we had a trigger\n",
    "                self.pulses.append(i)\n",
    "                self.letter.append(self.data.iloc[9][i])\n",
    "                if self.is_train() :\n",
    "                    self.label.append(self.data.iloc[10][i])\n",
    "                    if self.data.iloc[10][i] == 1 : \n",
    "                        self.True_pulses.append(i)\n",
    "    #-----------------------------------------------------------------------------------------------------                            \n",
    "    def get_performance(self):# if it performs in 'Row-Column' way or 'Single Character' one\n",
    "        performance = 'Not defined'\n",
    "        for i in range(0, self.data.shape[1]):\n",
    "\n",
    "            if self.data.iloc[9][i] > 12 :\n",
    "                performance = 'Single Character'\n",
    "                break\n",
    "        if performance == 'Not defined':\n",
    "            performance = 'Row-Column'\n",
    "        return performance\n",
    "    #-----------------------------------------------------------------------------------------------------\n",
    "    def get_time_feature(self, puls): # selceting time features \n",
    "        new_df = self.data.iloc[1:9, puls -25 : puls + 140]\n",
    "        signals = np.array(new_df).flatten() #all of singlas in 8 channels \n",
    "        mean_signal = new_df.mean() #mean singal \n",
    "        channel_mean = new_df.mean(axis = 1 ) #mean of each channel\n",
    "        elements_mean = channel_mean.mean() #mean of means \n",
    "        channel_std = new_df.std(axis =1) #standard deviation of each channel\n",
    "        elements_std = new_df.stack().std() #standard deviation of all elemnts\n",
    "        channel_corr = np.corrcoef(new_df)[np.tril_indices(8, k = -1)]# channels correlation\n",
    "        histogram = np.histogram(new_df, bins = 24, range=(-60,60))[0] #histogram of all channels\n",
    "        histogram_channel = []\n",
    "        for i in range(0,8):\n",
    "            histogram_channel.append(np.histogram(new_df.iloc[i], bins = 24, range=(-60,60))[0])#histogram of each channel\n",
    "        histogram_channel = np.array(histogram_channel).flatten()\n",
    "        features = np.concatenate((mean_signal, signals, channel_mean, channel_std, [elements_mean], [elements_std], histogram_channel , histogram, channel_corr))#put all features to gether\n",
    "        return features\n",
    "    #-----------------------------------------------------------------------------------------------------\n",
    "    def fft_features(self,puls):#getting frequency domain features\n",
    "        new_df = self.data.iloc[1:9, puls -25 : puls + 140]\n",
    "        \n",
    "        periodogram_df = periodogram(new_df)[1] #Estimate power spectral density using a periodogram.\n",
    "        power = periodogram_df[:,:30].flatten() \n",
    "        delta_energy = periodogram_df[:,1:2].sum(axis = 1) #delta energy\n",
    "        tetha_energy = periodogram_df[:,2:4].sum(axis = 1) #tetha energy\n",
    "        alpha_energy = periodogram_df[:,4:7].sum(axis = 1) #alpha energy\n",
    "        beta_energy = periodogram_df[:,7:15].sum(axis = 1) #beta energy\n",
    "        gamma_energy = periodogram_df[:,15:].sum(axis = 1) #gamma energy\n",
    "        sum_power_channel = periodogram_df[:,:30].sum(axis = 1) #sum of energies\n",
    "        sum_power = power.sum()\n",
    "        maximum_freqs_inx = (-abs(rfft(np.array(new_df)))).argsort()[:,:6].flatten()\n",
    "        maximum_freqs = rfftfreq(165 , 0.003906)[maximum_freqs_inx] #getting maximum freq\n",
    "        average_frequance = np.average([rfftfreq(165 , 0.003906)]*8 ,weights = abs(rfft(np.array(new_df))),axis =1) # computing weighted average\n",
    "        meadian_freq =[]\n",
    "        for i in range(0,8):\n",
    "            meadian_freq.append(weighted.median(rfftfreq(165 , 0.003906), abs(rfft(np.array(new_df.iloc[i])))))#computing weighted meadian\n",
    "        meadian_freq = np.array(meadian_freq).flatten()\n",
    "        features = np.concatenate((power, delta_energy, tetha_energy, alpha_energy, beta_energy, gamma_energy, sum_power_channel, [sum_power], maximum_freqs, average_frequance,meadian_freq))\n",
    "        return features\n",
    "    #--------------------------------------------------------------------------------------------------\n",
    "    def wavelet_features(self, puls): # getting wavelet features\n",
    "        new_df = self.data.iloc[1:9, puls -25 : puls + 140] \n",
    "        wavelet = pywt.wavedec(new_df, 'db1',level = 5) \n",
    "        mean_list= [np.mean(coeff) for coeffs in wavelet for coeff in coeffs] #\n",
    "        mode_list = [statistics.mode(coeff) for coeffs in wavelet for coeff in coeffs]\n",
    "        var_list =  [coeff.var() for coeffs in wavelet for coeff in coeffs]\n",
    "        std_list =  [coeff.std() for coeffs in wavelet for coeff in coeffs]\n",
    "        per5 =  [np.percentile(coeff,5) for coeffs in wavelet for coeff in coeffs]\n",
    "        per25 =  [np.percentile(coeff,25) for coeffs in wavelet for coeff in coeffs]\n",
    "        per75 = [np.percentile(coeff,75) for coeffs in wavelet for coeff in coeffs]\n",
    "        per95 =  [np.percentile(coeff,95) for coeffs in wavelet for coeff in coeffs]\n",
    "        zero_crossing = [len((np.nonzero(np.diff(coeff > 0)))[0]) for coeffs in wavelet for coeff in coeffs]\n",
    "        mean_crossing = [len((np.nonzero(np.diff(coeff > np.mean(coeff))))[0]) for coeffs in wavelet for coeff in coeffs]\n",
    "\n",
    "        features = np.concatenate((mean_list, mode_list, var_list, std_list, per5, per25, per75, per95, zero_crossing, mean_crossing))\n",
    "        return features \n",
    "    #--------------------------------------------------------------------------------------------------\n",
    "    def set_data(self): #for making data frame with samples and their features\n",
    "        new_data = pd.DataFrame()\n",
    "        data_array=[]\n",
    "        for puls in tqdm(self.pulses): \n",
    "            \n",
    "            time_feature = np.array(self.get_time_feature(puls)) \n",
    "            freq_feature = np.array(self.fft_features(puls))\n",
    "            wavelet_feature = np.array(self.wavelet_features(puls))\n",
    "            features = np.concatenate((time_feature, freq_feature, wavelet_feature))\n",
    "            data_array.append(features)\n",
    "            \n",
    "        feature_data = pd.DataFrame(data_array) \n",
    "\n",
    "        if self.is_train():\n",
    "            feature_data['label'] = np.array(self.label)\n",
    "\n",
    "        return feature_data\n",
    "    \n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "bf0c5492",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Person:\n",
    "    person_num: str\n",
    "    def __init__(self, person_num):\n",
    "        self.person_num = person_num\n",
    "        self.train = Data('TrainData', person_num)\n",
    "        self.test = Data('TestData', person_num)\n",
    "        self.performance = self.train.get_performance()\n",
    "    #--------------------------------------------------------------------------------------------------\n",
    "    def set_features(self): # getting features\n",
    "        self.train_features = self.train.set_data()\n",
    "        self.test_features = self.test.set_data()\n",
    "    #--------------------------------------------------------------------------------------------------    \n",
    "    def select_data(self , number_of_feature = [195,7]): # At first, we select a number of features with fisher score and then use PCA, and select best of it's compennet\n",
    "        fisher_score1 = get_fisher_score(self.train_features)\n",
    "        best_features_train1 = select_data(self.train_features, fisher_score1, number_of_feature[0])\n",
    "        best_features_test1 = select_data(self.test_features, fisher_score1, number_of_feature[0])\n",
    "        # then fit and trasform our data with PCA\n",
    "        transformer1 = IncrementalPCA(n_components=number_of_feature[0], batch_size=1000)\n",
    "        X_train_sparse = sparse.csr_matrix(best_features_train1)\n",
    "        X_train_transformed = pd.DataFrame(transformer1.fit_transform(X_train_sparse))\n",
    "        X_train_transformed['label'] = self.train.label\n",
    "        X_test_sparse = sparse.csr_matrix(best_features_test1)\n",
    "        X_test_transformed = pd.DataFrame(transformer1.transform(X_test_sparse))\n",
    "        #select best components \n",
    "        fisher_score2 = get_fisher_score(X_train_transformed)\n",
    "        self.final_train = select_data(X_train_transformed, fisher_score2, number_of_feature[1])\n",
    "        self.final_test = select_data(X_test_transformed, fisher_score2, number_of_feature[1])\n",
    "        self.final_train['label'] = self.train.label\n",
    "    #--------------------------------------------------------------------------------------------------    \n",
    "    def normalize(self):\n",
    "        self.normalized_train = (self.final_train - self.final_train.mean()) / self.final_train.std()\n",
    "        self.normalized_test = (self.final_test - self.final_train.mean()) / self.final_train.std()\n",
    "        self.normalized_train.drop('label', axis = 1)\n",
    "        self.normalized_train['label'] = self.train.label\n",
    "    #--------------------------------------------------------------------------------------------------             \n",
    "    def evalute_model(self, models , plotting = False):\n",
    "        X = self.normalized_train.drop('label', axis =1)\n",
    "        y = self.normalized_train.label\n",
    "        models_score = []\n",
    "        number_of_rows = int(len(models) // 4 + 1)\n",
    "\n",
    "        for i in range(len(models)):\n",
    "            rfc = models[i]\n",
    "            scores, cm = CV(X,y,rfc, 6)\n",
    "            cm = cm / cm.sum(axis = 1).reshape((2,1)) #normalize our confusion matrix\n",
    "            models_score.append([rfc] + list(scores)) \n",
    "            if plotting: #for plotting confusion matrix\n",
    "                disp = ConfusionMatrixDisplay(confusion_matrix=cm )\n",
    "                disp.plot()\n",
    "                print('Confusion Matrix for ' + str(rfc))\n",
    "                plt.show()\n",
    "        #now we will make a data frame and put the information abaut our models into it \n",
    "        models_score_df = pd.DataFrame(models_score)\n",
    "        models_score_df.columns = ['classifier' , 'accuracy', 'roc_auc', 'sensitivity', 'specificity' ]\n",
    "        models_score_df.sort_values(by=['roc_auc'] , inplace= True, ascending=False)\n",
    "        if plotting:\n",
    "            display(models_score_df)\n",
    "        self.best_model = models_score_df.iloc[0]['classifier']\n",
    "        return models_score_df.iloc[0]['roc_auc']\n",
    "    #--------------------------------------------------------------------------------------------------        \n",
    "    def finding_best_parameters(self):\n",
    "        best_roc = []\n",
    "        number_of_features = [[50,10],[95,7],[80,6],[70,7],[60,8],[55,9],[45,8]]\n",
    "        for i in range(len(number_of_features)):\n",
    "            self.select_data(number_of_feature = number_of_features[i])\n",
    "            self.normalize()\n",
    "            best_roc.append(self.evalute_model(models, plotting = False))\n",
    "        best_num_featrues = number_of_features[(-np.array(best_roc)).argsort()[0]] # find which number of features are the best\n",
    "        self.select_data(number_of_feature = best_num_featrues)\n",
    "        self.normalize()\n",
    "        self.evalute_model(models, plotting = True)\n",
    "        print('Best number of features are ', best_num_featrues)\n",
    "        print('Best model is ', self.best_model)\n",
    "    #--------------------------------------------------------------------------------------------------\n",
    "    def train_model(self): # now we should train our model on whole training set\n",
    "        rfc = self.best_model\n",
    "        X_train = self.normalized_train.drop('label', axis =1)\n",
    "        y_train = self.normalized_train.label\n",
    "        X_test = self.normalized_test.drop('label', axis =1)\n",
    "        y_test = self.normalized_test.label\n",
    "        \n",
    "        rfc.fit(X_train, y_train)\n",
    "        pred_train = rfc.predict(X_train)\n",
    "        pred_test = rfc.predict(X_test)\n",
    "        accuracy = rfc.score(X_train, y_train)\n",
    "        roc_auc = roc_auc_score(y_train, pred_train)\n",
    "        cm = confusion_matrix(y_train, pred_train)\n",
    "        TN, FP, FN, TP = cm.ravel()\n",
    "        sensitivity = TP / (TP + FN)\n",
    "        specificity = TN / (TN + FP)\n",
    "        filename = 'finalized_model' + str(self.person_num) + '.sav'\n",
    "        pickle.dump(rfc, open(filename, 'wb'))\n",
    "        best_model = [rfc , accuracy, roc_auc ,sensitivity, specificity]\n",
    "        best_model_information = pd.DataFrame(np.array(best_model).reshape(1,5))\n",
    "        best_model_information.columns = ['classifier' , 'accuracy', 'roc_auc', 'sensitivity', 'specificity' ]\n",
    "        print('scores for our best model fitted on whole training set is :')\n",
    "        display(best_model_information)\n",
    "        \n",
    "        prob_train = rfc.predict_proba(X_train)\n",
    "        prob_test = rfc.predict_proba(X_test)\n",
    "        return prob_train, prob_test\n",
    "    #--------------------------------------------------------------------------------------------------        \n",
    "    def word_detection(self): #detecting the word\n",
    "        prob_train, prob_test = self.train_model()\n",
    "        if self.performance == 'Single Character':\n",
    "            train_word = SC_WD(prob_train, self.train.letter)\n",
    "            test_word = SC_WD(prob_test, self.test.letter)\n",
    "            \n",
    "        if self.performance == 'Row-Column':\n",
    "            train_word = RC_WD(prob_train, self.train.letter)\n",
    "            test_word = RC_WD(prob_test, self.test.letter)\n",
    "        print('Training set word is ', train_word)\n",
    "        print('Test set word is ', test_word)"
   ]
  },
  {
